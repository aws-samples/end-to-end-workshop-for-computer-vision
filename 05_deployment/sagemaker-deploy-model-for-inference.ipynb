    "classes = [13, 17, 35, 36, 47, 68, 73, 87]\r\n",
    "\r\n",
    "possible_classes= cv_utils.get_classes_as_list(classes_file,classes)\r\n",
    "\r\n",
    "possible_classes"
   ],
   "outputs": [],
   "metadata": {}
